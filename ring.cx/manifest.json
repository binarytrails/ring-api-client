{
    "name": "Ring.cx",
    "description": "",
    "version": "1.0",
    "manifest_version": 2,
    "homepage_url": "https://github.com/sevaivanov/ring-webextension",
    "icons": {
        "48": "res/icons/ring.svg"
    },

    "applications": {
        "gecko": {
            "id": "client@ring.cx",
            "strict_min_version": "1.0"
        }
    },

    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_icon": "res/icons/ring.svg",
        "default_title": "Ring.cx",
        "default_popup": "popup/contacts.html"
    },

    "web_accessible_resources": []
}
